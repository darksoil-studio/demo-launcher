{
  "name": "demo-launcher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "AGENTS=2 npm run network",
    "network": "pnpm build:happ-store && concurrently -k \"npm run dev\" \"npm run launch\"",
    "network:android": "concurrently -k \"npm run tauri dev\" \"npm run tauri android dev\"",
    "build:happ-store": "nix build .#happ-store -o happ-store.webhapp",
    "launch": "concurrently-repeat \"npm run tauri dev\" $AGENTS",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@holochain/client": "^0.18.0",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": ">=2.0.0",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "concurrently": "^9.1.2",
    "concurrently-repeat": "^0.0.1",
    "internal-ip": "^7.0.0",
    "internal-ip-cli": "^2.0.0",
    "new-port-cli": "^1.0.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  }
}
