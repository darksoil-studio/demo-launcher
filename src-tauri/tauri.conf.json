{
  "productName": "Demo Launcher",
  "version": "0.2.13",
  "identifier": "studio.darksoil.demolauncher",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "capabilities": [
        "mobile-capability",
        "main-capability",
        "desktop-capability"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "entitlements.plist"
    },
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDE3RUU3OTAxOUYzN0M2RkEKUldUNnhqZWZBWG51RitCWXhZdFY4RHVhQzY0bVgxU0tnc0JBSm9pSmliZ2pBUGRvOWFlbWV6b24K",
      "endpoints": [
        "https://github.com/darksoil-studio/demo-launcher/releases/latest/download/latest.json"
      ]
    }
  }
}
